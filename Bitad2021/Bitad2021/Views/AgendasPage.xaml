<?xml version="1.0" encoding="utf-8"?>

<xamForms:ReactiveTabbedPage x:TypeArguments="viewModels:AgendasViewModel" xmlns="http://xamarin.com/schemas/2014/forms"
                              xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                              xmlns:xamForms="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                              xmlns:viewModels="clr-namespace:Bitad2021.ViewModels;assembly=Bitad2021"
                              xmlns:views="clr-namespace:Bitad2021.Views;assembly=Bitad2021"
                              x:Class="Bitad2021.Views.AgendasPage">
    
    <views:LecturesPage Title="Wykłady" BindingContext="{Binding LecturesViewModel}"/>
    <views:WorkshopsPage Title="Warsztaty" BindingContext="{Binding WorkshopsViewModel}"/>
    
</xamForms:ReactiveTabbedPage>